{

    "manifest_version": 2,
    "name": "Tab Counter",
    "version": "1.1",
  
    "browser_specific_settings": {
      "gecko": {
        "id": "content-script-example@mozilla.org",
        "strict_min_version": "59.0a1"
      }
    },
  
    "description": "Demonstration of contentScripts.register.",
  
    "permissions": [
      "<all_urls>"
    ],

    "background": {
      "scripts": ["background.js"]
    }
  
  }
  